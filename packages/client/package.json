{
  "name": "@bangumi/client",
  "version": "0.0.0",
  "type": "module",
  "description": "http client for private api",
  "main": "./index.ts",
  "private": true,
  "scripts": {
    "update-openapi": "node scripts/update-openapi.mjs",
    "build": "(oazapfts openapi.json > client.ts) && prettier -w ./",
    "build2": "(oazapfts http://127.0.0.1:4000/p1/openapi.json > client.ts) && prettier -w ./"
  },
  "dependencies": {
    "oazapfts": "^4.5.0"
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.9",
    "@faker-js/faker": "^7.6.0",
    "change-case": "^4.1.2",
    "js-yaml": "^4.1.0",
    "msw": "^0.49.0",
    "node-fetch": "^3.3.0",
    "openapi-types": "^12",
    "openapi-typescript": "^6.1.0",
    "orval": "^6.10.3"
  }
}
